{
  "name": "promptius-gui-monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "The frontend code to support dynamic UI generation by Large Language Models",
  "keywords": [
    "generative-ui",
    "llms",
    "react"
  ],
  "author": "Kanishk Gupta <kanishkgupta2000@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgentBossMode/promptius-gui.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "generate": "../scripts/generate-all.sh",
    "generate:watch": "nodemon --watch ../schema/promptius-gui-schema.json --exec \"../scripts/generate-all.sh\"",
    "prebuild": "npm run generate",
    "build": "npm run build -ws",
    "dev": "npm run build && vite"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "json-schema-to-typescript": "^14.1.0",
    "json-schema-to-zod": "^1.0.0",
    "lucide-react": "^0.546.0",
    "nodemon": "^3.0.0",
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "^4.1.14",
    "typescript": "^4.5.0",
    "vite": "^7.1.10",
    "zod": "^3.22.0"
  },
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/vite": "^4.1.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.18.2",
    "react-hot-toast": "^2.6.0",
    "react-syntax-highlighter": "^15.6.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  }
}
